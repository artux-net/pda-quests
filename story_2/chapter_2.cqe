    {
      "id": 236,
      "type_stage": 0,
      "background_url": "story/freeplay/screen/48.jpg",
      "title": "Военный",
      "message": "",
      "type_message": 0,
      "texts": [
        {
          "text": "Стоять! Кто такой?",
          "condition": {}
